{
	"name": "nx-utils",
	"scripts": {
		"build": "turbo run build",
		"docs:dev": "turbo run docs:dev --filter=apps-docs",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"lint": "turbo run lint",
		"prepare": "husky"
	},
	"devDependencies": {

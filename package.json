		"@commitlint/config-conventional": "^19.8.0",
		"@nx-utils/eslint-config": "workspace:*",
		"@nx-utils/prettier-config": "workspace:*",
		"eslint": "^8.57.0",
		"husky": "^9.1.7",
		"prettier": "^3.2.5",
		"turbo": "^2.5.0"
	},
	"packageManager": "pnpm@8.15.6"
}

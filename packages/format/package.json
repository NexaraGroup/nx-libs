	},
	"devDependencies": {
		"@nxlibs/eslint-config": "workspace:*",
		"@nxlibs/typescript-config": "workspace:*",
		"@types/big.js": "^6.0.0",
		"tsup": "^8.0.0"
	},
	"peerDependencies": {
		"big.js": "^6.0.0",
		"dayjs": "^1.11.0"
	}
